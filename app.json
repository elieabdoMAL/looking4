{
  "expo": {
    "name": "looking4",
    "slug": "mobileappslabs-looking4",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/square-logo.png",
    "scheme": "looking4",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/square-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mobileappslabs.mobileappslabslooking4"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/square-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/square-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0f41a157-b86d-4dab-9fe6-3c3a3ae0409a"
      }
    },
    "owner": "mobileappslabs"
  }
}
